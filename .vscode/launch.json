        "program": "${workspaceFolder}/build/src/Pink/Pink",
        "args": ["--train", "data.bin", "som.bin"],
        "stopAtEntry": false,
        "cwd": "/home/bernd/pink/test-3d",
        "environment": [],
        "externalConsole": false,
        "MIMode": "gdb",
        "setupCommands": [
            {
                "description": "Enable pretty-printing for gdb",
                "text": "-enable-pretty-printing",
                "ignoreFailures": true
            }
        ]
    },
        {
            "name": "(gdb) Launch",
            "type": "cppdbg",
